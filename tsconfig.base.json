{
  "compilerOptions": {
    "target": "esnext",
    "composite": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "paths": {
      "@utk/sdk-core": ["sdk-core/src"],
      "@utk/sdk-core/*": ["sdk-core/src/*"],
      "@utk/sdk-router": ["sdk-router/src"],
      "@utk/sdk-router/*": ["sdk-router/src/*"],
      "@utk/sdk-api": ["sdk-api/src"],
      "@utk/sdk-api/*": ["sdk-api/src/*"],
    },
    "esModuleInterop": true,
    "skipLibCheck": true
  }
 }
